extends layout

block content
	nav(class='navbar navbar-custom navbar-fixed-top', role='navigation')
		div(class='container', style='margin-right: 0;width: 100%')
			div(class='navbar-header visible-xs')
				div(class='mini-logobouineur')
			//Collect the nav links, forms, and other content for toggling -->
			div(class='navbar visible-xs visible-sm navbar-custom-phone')
				div(class='dropdown-custom')
					a(href='#'	,data-toggle='dropdown', class='dropdown-toggle')
						span(class='glyphicon glyphicon-menu-hamburger',aria-hidden='true')
						Menu
					ul(class='dropdown-menu')
						li(class='hidden')
							a(href='#page-top')
						li
							a(class='page-scroll', href='#home') Home
						li(role='separator',class='divider')
						li
							Le Projet
							li
								a(class='page-scroll',href='#quatreL') Le 4L Trophy
							li
								a(class='page-scroll',href='#equipage') L'équipage
							li
								a(class='page-scroll',href='#voiture') La voiture
						li(role='separator',class='divider')
						li
							a(class='page-scroll', href='#onlive') On Live&iexcl;
						li(role='separator',class='divider')
						li
							a(class='page-scroll', href='#sponsors') Sponsors
			div(class='navbar-right hidden-xs hidden-sm col-md-10 col-lg-10')
				ul(class='nav navbar-nav navbar-bar-custom')
					// Hidden li included to remove active class from about link when scrolled up past about section
					li(class='hidden')
							a(href='#page-top')
					li
						a(class='page-scroll', href='../#home') Home
					li(role='separator',class='divider')
					li
						a(class='page-scroll', href='../#projet') Le Projet
					li(role='separator',class='divider')
					li
						a(class='page-scroll', href='../#onlive') On Live&iexcl;
					li(role='separator',class='divider')
					li
						a(class='page-scroll', href='../#sponsors') Sponsors
	header(id='onlive-page',class='intro')
		div(class'intro-body')
			div(class='container')
				h1 ON LIVE
				h3 #{fullDateString}
				div(class='row' style='height:120px')
					div(class='col-md-4 col-lg-4 blockImg')
						img(src="/static/images/maps.png")
					div(class='col-md-4 col-lg-4 blockLien')
						button(type='button' class='btn btn-default btn-lg')
							a(class='glyphicon  glyphicon-road' aria-hidden='true' href='../onlive/maps/#{key}') Voir l'itinéraire du jour
					div(class='col-md-4 col-lg-4 blockIcon')
						img(src="/static/images/iconMap.png")
				div(class='row')
					div(class='blockOnlive')
						div(class='col-md-3 col-lg-3 blockImg')
						div(class='col-md-3 col-lg-3 blockIcon iconPhoto')
						div(class='col-md-3 col-lg-3 blockLien')
							button(type='button' class='btn btn-default btn-lg')
								span(class='glyphicon glyphicon-camera' aria-hidden='true' href='../onlive/photos/#{key}')    Voir les photos du jour
				div(class='row')
					div(class='blockOnlive')
						div(class='col-md-3 col-lg-3 blockImg')
						div(class='col-md-3 col-lg-3 blockIcon iconCarte')
						div(class='col-md-3 col-lg-3 blockLien')
							span(class='') Text Random here