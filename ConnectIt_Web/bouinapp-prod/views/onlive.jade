extends layout

block content
	nav(class='navbar navbar-custom navbar-fixed-top', role='navigation')
			div(class='container', style='margin-right: 0;width: 100%')
				div(class='navbar-header visible-xs')
					div(class='mini-logobouineur')
				div(class='navbar visible-xs visible-sm navbar-custom-phone')
					div(class='dropdown-custom')
						a(href='#', data-toggle='dropdown', class='dropdown-toggle')
							span(class='glyphicon glyphicon-menu-hamburger',aria-hidden='true') Menu
						ul(class='dropdown-menu')
							li(class='hidden')
								a(href='#page-top')
							li
								a(class='page-scroll', href='/#home') Home
							li(role='separator',class='divider')
							li
								a(class='page-scroll', href='/#projet') Projet
							li(role='separator',class='divider')
							li
								a(class='page-scroll', href='/#onlive') On Live&iexcl;
							li(role='separator',class='divider')
							li
								a(class='page-scroll', href='/#sponsors') Sponsors
				div(class='navbar-right hidden-xs hidden-sm col-md-10 col-lg-10')
					ul(class='nav navbar-nav navbar-bar-custom')
						li(class='hidden')
								a(href='#page-top')
						li
							a(class='page-scroll', href='/#home') Home
						li
							a(class='page-scroll', href='/#projet') Le Projet
						li(class='active')
							a(class='page-scroll', href='/#onlive') On Live&iexcl;
						li
							a(class='page-scroll', href='/#sponsors') Sponsors
	header(id='onlive-page',class='intro')
		div(class'intro-body')
			div(class='container')
				h3 #{fullDateString}
				div(class='row' style='height:120px')
					div(class='col-md-3 col-lg-3 nopadding')
						ul(class='onlive-alternative')
							li(class='content active')
								a(href='/onlive/maps/#{key}')
									span(style='display: inline-block; vertical-align: middle; margin:5px 0 0 15px; float:left')
										img(class='icon-trace', src='/static/images/iconMap.png')
									span(style='font-size:1.7em; display: inline-block; vertical-align: middle; margin:15px 0 0 0px') LE TRACÉ
									span(style='height:100%; display: inline-block; vertical-align: middle; background:black;	 width: 10px; float: right;')
							li(class='empty')
							li(class='content')
								a(href='/onlive/timelaps/#{key}')
									span(style='display: inline-block; vertical-align: middle; margin:5px 0 0 15px; float:left')
										img(class='icon-timelaps', src='/static/images/video.png')
									span(style='font-size:1.7em; display: inline-block; vertical-align: middle; margin:15px 0 0 0px') TIMELAPS
					div(class='col-md-9 col-lg-9')
						h1 Vous vous êtes perdus ?!
						h2 
							a(href="/") GO BACK HOME :)